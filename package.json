{
  "name": "tattoo-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "graphql-codegen --config graphql.config.ts && next dev --turbopack",
    "build": "graphql-codegen --config graphql.config.ts && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "build:docker": "next build",
    "build:check": "next build",
    "type-check": "tsc --noEmit",
    "codegen": "graphql-codegen --config graphql.config.ts",
    "codegen:watch": "graphql-codegen --config graphql.config.ts --watch",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@apollo/client": "^3.14.0",
    "@apollo/server": "^5.0.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-imask": "^7.6.1",
    "tailwind-merge": "^3.3.1",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0"
    ]
  }
}
